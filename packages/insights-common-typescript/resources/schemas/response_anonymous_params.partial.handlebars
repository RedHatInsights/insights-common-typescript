{{#if responses}}
    {{~#each responses ~}}
        {{~#with content.[application/json]~}}
            {{~#unless this.schema.[$ref]~}}
                const AnonymousZodSchema{{className ../../name}}{{@key}} = {{#if (eq? this.schema.type "object") }} z.object({ {{/if~}} {{~>zodschema this.schema namespace="schemas"~}} {{~#if (eq? this.schema.type "object") ~}} }) {{~/if~}};
            {{/unless}}
        {{/with}}
    {{/each}}
{{/if~}}

{{#*inline "_response_anonymous_params"}}
export interface {{>request_params_name .}} {
    {{#if (deref requestBody @root)}}
    {{#with (deref requestBody @root)}}body{{#unless required}}?{{/unless}}: {{#first content}}{{>schema schema namespace="schemas"}};{{/first}}{{/with}}
    {{/if}}
    {{#each parameters}}
        {{#with (deref this @root)}}
            {{#unless (eq? in "cookie")}}
    {{valueName name}}{{#unless required}}?{{/unless}}: {{>schema (coalesce schema content.schema) namespace="schemas"}};
{{/unless}}{{/with}}{{/each~}}
}
{{/inline}}
